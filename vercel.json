{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "crons": [
    {
      "path": "/api/servers/cron/v1",
      "schedule": "0 0-23/1 * * *"
    },
    {
      "path": "/api/servers/cron",
      "schedule": "0 01 * * *"
    }
  ]
}
